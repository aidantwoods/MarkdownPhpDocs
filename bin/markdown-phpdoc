#!/usr/local/bin/php
<?php

namespace Aidantwoods\MarkdownPhpDocs;

const AUTOLOAD = 'vendor/autoload.php';

$dir = __DIR__.'/../';

if (is_file($dir.'../../../'.AUTOLOAD))
{
    $dir .= '../../../';
}

require_once($dir.AUTOLOAD);

$MarkdownPhpDocs = new MarkdownPhpDocs;
$MarkdownPhpDocs->generateMdFiles();
