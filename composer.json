{
    "name": "aidantwoods/markdownphpdocs",
    "description": "Markdown Documentation Generator for PHP",
    "license": "MIT",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "aidantwoods/phpDocumentor2",
                "version":"dev-no-markdown",
                "source": {
                    "url": "https://github.com/aidantwoods/phpDocumentor2",
                    "type": "git",
                    "reference":"no-markdown"
                }
            }
        }
    ],
    "autoload": {
        "psr-4":{
            "Aidantwoods\\MarkdownPhpDocs\\": ["src/"]
        }
    },
    "require": {
        "php": ">=5.3",
        "aidantwoods/phpDocumentor2": "dev-no-markdown"
    }
}
