    "description": "Markdown Documentation Generator for PHP",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aidantwoods/phpDocumentor2"
        }
    ],
    "autoload": {
        "psr-4":{
            "Aidantwoods\\MarkdownPhpDocs\\": ["src/"]
        }
