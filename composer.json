{
    "name": "aidantwoods/markdownphpdocs",
    "description": "Markdown Documentation Generator for PHP",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
            "name": "aidantwoods/phpdocumentor",
            "version": "dev-no-markdown",
                "source": {
                    "url": "https://github.com/aidantwoods/phpDocumentor2",
                    "type": "git",
                    "reference": "no-markdown"
                }
            }
        }
    ],
    "autoload": {
        "psr-4":{
            "Aidantwoods\\MarkdownPhpDocs\\": ["src/"]
        }
    },
    "require": {
        "php": ">=7.0",
        "aidantwoods/phpdocumentor": "dev-no-markdown",
        "aidantwoods/betteroptions": "dev-master",
        "symfony/yaml": "^3.2"
    },
    "bin": ["bin/markdown-phpdoc"]
}
